<view class='container'>
  <view class='header'>
    <swiper style="width: 750rpx;height: 490rpx;" indicator-dots="true" autoplay="true" interval="3000" duration="500" circular="true">
        <swiper-item>
          <image class='bg-img' src='../../images/BANNER1.png'></image>
          
        </swiper-item>
         <swiper-item>
          <image class='bg-img' src='../../images/BANNER2.png'></image>
          
        </swiper-item>
    </swiper>
    <!-- <image class='bg-img' src='../../images/BANNER1.png'></image> -->
    <input class='search' bindtap="searchcountry"></input>
    <image class='search-img' bindtap="searchcountry" src='../../images/icn_disocver_active.png'></image>
    <text bindtap="searchcountry" class='tips'>搜索国家/地区</text>
    <image bindtap="gotoselfcenter" class='personal-center' src='../../images/个人中心.png'></image>
  </view>
  <view class="problem-main">
    <image bindtap="gotoproblemlist" class='problem-img' src='../../images/problem.png'></image>
    <text bindtap="gotoproblemlist" class='problem-text1'>常见问题</text>
    <swiper class="swiper_container" vertical="true" autoplay="true" circular="true" interval="2000">
      <block wx:for="{{problemlunbo}}" wx:key='index'>
        <swiper-item>
          <view bindtap="thisproblemdetail" data-index="{{index+1}}" data-id="{{item.Id}}" class="swiper_item">
            {{item.Problem}}
          </view>
        </swiper-item>
      </block>
    </swiper>
    <text bindtap="gotoproblemlist" class='problem-text2'>更多</text>
    <image bindtap="gotoproblemlist" class='nextstep1' src='../../images/下一步.png'></image>
  </view>
  <view id='list' style="position: sticky;top:0;position: -webkit-sticky;z-index:999;width:750rpx">
    <view class='tabswitchcontainer'>
      <block wx:for="{{tabswitch}}" wx:key='index'>
        <text id="{{item.id}}" bindtap="tabclick" data-select="{{item.select}}" class="{{item.select==catalogSelect?'select':''}}">
          {{item.tabname}}
        </text>
      </block>
    </view>
  </view>
  <view class='main'>
    <scroll-view scroll-y="true" style="height: 100%;" scroll-into-view="two">
      <view id="one">
        <view class='title'>
          <text bindtap="goTop" class='title1'>热门国家和地区</text>
          <text class='title2'>Hot country or region</text>
        </view>
        <view class='country-list'>
          <view class='img-box' wx:for="{{hotdata}}" wx:for-item="item" data-id="{{item.Id}}" wx:key='index'>
            <image bindtap='gotodetails' data-id="{{item.Id}}" class='country-list-img' src="{{item.HomeImage}}"></image>
            <view class="gotodetails"></view>
            <view class="smallbox">
              <text class="gotodetails-name">{{item.Country_ZH}}</text>
              <text data-id="{{item.Id}}" bindtap='gotodetails' class="gotodetails-name1">
                查看详情
              </text>
            </view>
            <view class='country-list-btn'>
              <text class='country'>{{item.Country_ZH}}</text>
              <button data-name="{{item.Country_ZH}}" size="mini" open-type="getUserInfo" bindgetuserinfo="getUserInfo">
                立即咨询
              </button>
            </view>
          </view>
        </view>
      </view>
      <view id="two">
        <view class='title'>
          <text bindtap='tapqqq' class='title1'>亚洲</text>
          <view>
            <view class='title3'>
              <text style='display:inline-block;margin-left:130rpx'>Asia</text>
              <view bindtap="gotocountrylistasia" style='float:right'>
                <text class='more-country'>更多国家</text>
                <image class='nextpage more-country' src='../../images/下一步.png'></image>
              </view>
            </view>
          </view>
        </view>
      </view>
      <scroll-view scroll-x="true">
        <view scroll-x="true" class='country-list-asia'>
          <block wx:for="{{asiadata}}" wx:key='index' wx:for-item="item">
            <view class='img-box-asia' data-id="{{item.Id}}">
              <image data-id="{{item.Id}}" bindtap='gotodetails' class='country-list-img' src="{{item.HomeImage}}"></image>
              <view class='topposition'>
                <view class='toppositioncountry'>{{item.Country_ZH}}</view>
                <button data-name="{{item.Country_ZH}}" size="mini" open-type="getUserInfo" bindgetuserinfo="getUserInfo">
                  立即咨询
                </button>
                <view data-id="{{item.Id}}" bindtap='gotodetails' class='toppositiondetail'>
                  查看详情
                </view>
              </view>
            </view>
          </block>
        </view>
      </scroll-view>
      <view id="three">
        <view class='title'>
          <text bindtap='tapqqq' class='title1'>美洲</text>
          <view>
            <view class='title3'>
              <text style='display:inline-block;margin-left:130rpx'>America</text>
              <view bindtap="gotocountrylistamerica" style='float:right'>
                <text class='more-country'>更多国家</text>
                <image class='nextpage more-country' src='../../images/下一步.png'></image>
              </view>
            </view>
          </view>
        </view>
