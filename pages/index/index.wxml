      </view>
      <scroll-view scroll-x="true">
        <view scroll-x="true" class='country-list-asia'>
          <block wx:for="{{oceaniadata}}" wx:for-item="item" wx:key='index'>
            <view class='img-box-asia' data-id="{{item.Id}}">
              <image data-id="{{item.Id}}" bindtap='gotodetails' class='country-list-img' src="{{item.HomeImage}}"></image>
              <view class='topposition'>
                <view class='toppositioncountry'>{{item.Country_ZH}}</view>
